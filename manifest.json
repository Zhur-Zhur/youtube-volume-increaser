{
  "manifest_version": 2,
  "name": "Increaser",
  "version": "1.0",

  "description": "Adds a volume multiplier to youtube video controls",

  "icons": {
    "48": "volume.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["increaser.js"]
    }
  ]
  
